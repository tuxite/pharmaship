msgid "Something went wrong! See detailed logs."
msgstr "Quelque chose s'est mal passé ! Consulter les journaux."

#: pharmaship/core/gpg.py:48
msgid "Key already added"
msgstr "Clé déjà ajoutée"

#: pharmaship/core/gpg.py:53
msgid "Key successfully added"
msgstr "Clé ajoutée avec succès"

#: pharmaship/core/gpg.py:114
msgid "Key not found!"
msgstr "Clé introuvable !"

#: pharmaship/core/gpg.py:117
msgid "Key removed"
msgstr "Clé retirée"

#: pharmaship/core/gpg.py:133
msgid "No signature found."
msgstr "Signature absente."

