#: pharmaship/core/import_data.py:183 pharmaship/core/import_data.py:187
#: pharmaship/core/import_data.py:194 pharmaship/core/import_data.py:198
msgid "File impossible to read. See detailled log."
msgstr "Fichier impossible à lire. Consulter les journaux."

#: pharmaship/core/import_data.py:213
msgid "Signature not validated. See detailled log."
msgstr "Signature invalide. Consulter les journaux."

#: pharmaship/core/import_data.py:240
msgid "Signed data is not a valid Tar file. Check logs."
msgstr ""
"Les données signées ne sont pas un fichier Tar valide. Vérifier les journaux."

#: pharmaship/core/import_data.py:248
msgid "Integrity check failed. Check logs."
msgstr "Échec de la vérification d'intégrité. Consulter les journaux."

#: pharmaship/core/import_data.py:253
msgid "Configuration not found."
msgstr "Configuration introuvable."

#: pharmaship/core/import_data.py:262
msgid "Configuration file incorrect. "
msgstr "Fichier de configuration incorrect."

#: pharmaship/core/import_data.py:265
msgid "Package Author"
msgstr "Auteur du paquet"

#: pharmaship/core/import_data.py:266
msgid "Package Version"
msgstr "Version du paquet"

#: pharmaship/core/import_data.py:267
msgid "Package Date"
msgstr "Date du paquet"

#: pharmaship/core/import_data.py:275
msgid "A module to update is not installed. Check logs for details."
msgstr ""
"Un module à mettre à jour n'est pas installé. Consulter les journaux pour "
"plus de détail."

